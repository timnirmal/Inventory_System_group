<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="406.0" stylesheets="@../styles/style.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.frames.staff.StaffController">
   <children>
      <Label layoutX="158.0" prefHeight="82.0" prefWidth="90.0" stylesheets="@../styles/Title.css" text="Staff" textFill="#383a4a">
         <font>
            <Font name="Comic Sans MS" size="30.0" />
         </font>
      </Label>
      <VBox layoutX="106.0" layoutY="103.0" prefHeight="254.0" prefWidth="194.0">
         <children>
            <Button fx:id="newItem" mnemonicParsing="false" onAction="#addStaffOnAction" prefHeight="59.0" prefWidth="193.0" text="Add Staff" textFill="#c21f35">
               <font>
                  <Font name="Comic Sans MS" size="20.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="30.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="showItems" mnemonicParsing="false" onAction="#showStaffOnAction" prefHeight="59.0" prefWidth="193.0" text="Show Staff" textFill="#bf1717">
               <font>
                  <Font name="Comic Sans MS" size="20.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="30.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="newCategory" mnemonicParsing="false" onAction="#editOnAction" prefHeight="59.0" prefWidth="193.0" text="Edit Staff" textFill="#b51111">
               <font>
                  <Font name="Comic Sans MS" size="20.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
